  ids = {karanth_estimation_2004-1,karanth_estimation_2004-2,karanth_estimation_2004-3},
  title = {Estimation of Tiger Densities in the Tropical Dry Forests of {{Panna}}, {{Central India}}, Using Photographic Capture–Recapture Sampling},
  author = {Karanth, K Ullas and Chundawat, Raghunandan S and Nichols, James D and Kumar, N Samba},
  date = {2004-08},
  journaltitle = {Animal Conservation},
  volume = {7},
