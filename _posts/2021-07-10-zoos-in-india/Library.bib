@article{sapolsky_social_2004,
  ids = {sapolsky_social_2004-1},
  title = {Social {{Status}} and {{Health}} in {{Humans}} and {{Other Animals}}},
  author = {Sapolsky, Robert M.},
  date = {2004-10},
  journaltitle = {Annual Review of Anthropology},
  volume = {33},
  pages = {393--418},
  issn = {0084-6570},
  doi = {10.1146/annurev.anthro.33.070203.144000},
  url = {http://www.annualreviews.org/doi/10.1146/annurev.anthro.33.070203.144000},
  abstract = {Dominance hierarchies exist in numerous social species, and rank in such hierarchies can dramatically influence the quality of an individual’s life. Rank can dramatically influence also the health of an individual, particularly with respect to stress-related disease. This chapter reviews first the nature of stress, the stress- response and stress-related disease, as well as the varieties of hierarchical systems in animals. I then review the literature derived from nonhuman species concerning the connections between rank and functioning of the adrenocortical, cardiovascular, reproductive, and immune systems. As shown here, the relationship is anything but monolithic. Finally, I consider whether rank is a relevant concept in humans and argue that socioeconomic status (SES) is the nearest human approximation to social rank and that SES dramatically influences health.},
  file = {/home/sitendu/zoterostorage/storage/Y83D25LB/9_sapolsky_2004_stress.pdf},
  keywords = {an individual,and rank in,exist in numerous social,glucocorticoids,i abstract dominance hierarchies,influence the quality of,nonhuman primates,psychoneuroimmunology,rank,s life,socioeconomic status,species,stress,such hierarchies can dramatically},
  number = {1}
}

@article{sapolsky_stress_1990,
  ids = {sapolsky_stress_1990-2},
  title = {Stress in the Wild.},
  author = {Sapolsky, Robert M},
  date = {1990},
  journaltitle = {Scientific American},
  volume = {262},
  pages = {116--123},
  issn = {0036-8733 (Print)\textbackslash r0036-8733 (Linking)},
  doi = {10.1038/scientificamerican0190-116},
  abstract = {The year was 1936, Hans Selye, a young physician just starting off in research at McGill University in Montreal, had a major problem. He had been injecting rats daily with a chemical extract to determine the extract’s effects and had identified consistent changes in the animals: peptic ulcers, atrophy of immune-system tissues and enlargement of the adrenal glands. To his surprise, however, the rats in the control group, which had been injected with saline solution alone, showed identical changes. Most scientists would have thrown up their hands at this paradox. Instead Selye focused on what the two groups had in common: the repeated injections. He wondered if the trio of changes he had identified was actually a generalised physiological response to unpleasantness per se. He then tested that idea and found the same three effects regardless of whether the rats were made too hot or too cold or were exposed to pathogens, toxins or loud noises. Selye borrowed a term from engineering to describe the body’s nonspecific response to an insult. What the rats were undergoing, he decided, was stress. Thus the field of stress physiology was born.},
  file = {/home/sitendu/zoterostorage/storage/IAVQ65P8/9_Sapolsky_1990.pdf},
  number = {1}
}

