@article{brooks_unraveling_2008,
  title = {Unraveling the Mechanisms of {{REM}} Sleep Atonia.},
  author = {Brooks, Patricia L and Peever, John H},
  date = {2008},
  journaltitle = {Sleep},
  volume = {31},
  pages = {1492--7},
  issn = {0161-8105 (Print)\textbackslash n0161-8105 (Linking)},
  abstract = {FOR THEIR DISCUSSION OF OUR WORK. 7 WE AGREE WITH THE COMMENTATORS that determining mechanisms of rapid eye movement (REM) sleep motor atonia is of major scientific importance and clini-cal relevance. The concept that REM atonia is under the con-trol of one physiological mechanism and one neurotransmitter pathway has seduced many of us. The undoubted appeal of the current theory—that glycinergic inhibition of motoneurons is the only mechanism mediating REM atonia—is its inher-ent simplicity. However, the fact that several laboratories now provide evidence indicating that glycinergic inhibition plays only a minimal role in mediating REM atonia 7-9 and that other neurotransmitter pathways contribute to motor suppression in REM sleep 10-13 indicates that we need to reevaluate mechanisms of REM atonia. If glycinergic inhibition is indeed the dominat-ing force driving motor inhibition during REM sleep, then why does blockade of glycine receptors at the trigeminal motor pool not prevent REM atonia in masseter muscles? 7 Our work demon-strates that antagonism of glycine and GABA A receptors at the trigeminal motor pool causes a profound increase in masseter muscle tone during both wakefulness and non-rapid eye move-ment (NREM) sleep; however, this same intervention does not prevent or even reverse REM atonia, despite the fact that it po-tently provokes muscle-twitch activity in REM sleep (Figure 1). In fact, REM atonia persists even when glycine and GABA A receptors are blocked and potent glutamatergic agonists are simul-taneously applied to the trigeminal motor pool. We interpret these findings to indicate that (1) glycinergic-and GABA A -mediated inhibition plays a minimal role in triggering REM atonia; (2) inhibition primarily functions to suppress muscle twitches dur-ing REM sleep; and, (3) a powerful, yet unidentified, inhibitory mechanism or mechanisms override excitation during REM sleep. Some commentators offer alternative possibilities for our experimental observations; we respond to these discussion points below.},
  eprint = {19226735%5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2579970},
  eprinttype = {pubmed},
  keywords = {\#nosource},
  number = {11}
}

