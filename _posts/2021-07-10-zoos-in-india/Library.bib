  file = {/home/sitendu/zoterostorage/storage/J7DCMEYI/groshong2014.pdf},
  keywords = {Borrelia,Borreliosis,Genetics,Lyme disease,Molecular biology,Mutagenesis,Pathogenesis,Regulation,Tick,Vector-borne}
}

