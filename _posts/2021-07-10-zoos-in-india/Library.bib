@article{guldenpfennig_cortico-striatal_2011,
  ids = {guldenpfennig_physiology_2011},
  title = {Cortico-Striatal Oxidative Status, Dopamine Turnover and Relation with Stereotypy in the Deer Mouse.},
  author = {Güldenpfennig, Marianne and Wolmarans, De Wet and Jan, L and Stein, Dan J and Harvey, Brian H and du Preez, Jan L and Stein, Dan J and Harvey, Brian H},
  date = {2011-06},
  journaltitle = {Physiology \& behavior},
  volume = {103},
  pages = {404--11},
  doi = {10.1016/j.physbeh.2011.03.008},
  abstract = {The deer mouse presents with spontaneous stereotypic movements that resemble the repetitive behaviours of obsessive-compulsive disorder (OCD), and demonstrates a selective response to serotonin reuptake inhibitors. OCD has been linked to altered redox status and since increased dopamine signalling can promote stereotypies as well as oxidative stress, we investigated whether the severity of deer mouse stereotypy may be associated with altered dopamine turnover and cortico-striatal redox status. Deer mice were separated into high (HSB), low (LSB) and non-stereotypy (NS) groups. Frontal cortical and striatal dopamine, 3,4-dihydroxyphenylacetic acid (DOPAC) and homovanillic acid (HVA), as well as superoxide dismutase (SOD) activity, reduced (GSH) and oxidised (GSSG) glutathione and glutathione redox index, were analysed as markers for regional dopamine turnover and oxidative stress, respectively. Dopamine and its metabolites and SOD activity did not differ across the stereotypy groups. Significantly reduced GSH and GSSG and increased glutathione redox index were only observed in the frontal cortex of HSB animals. Frontal cortical GSH and GSSG were inversely correlated while glutathione redox index was positively correlated with stereotypy. Deer mouse stereotypy is thus characterised by a deficient glutathione system in the frontal cortex but not striatum, and provides a therapeutic rationale for using glutathione-active antioxidants in OCD. The evidence for a primary frontal lesion has importance for future OCD research.},
  eprint = {21397620},
  eprinttype = {pubmed},
  file = {/home/sitendu/zoterostorage/storage/388P5WFL/1-s2.0-S0031938411001259-main.pdf;/home/sitendu/zoterostorage/storage/VL4DJLSN/1-s2.0-S0031938411001259-main.pdf},
  keywords = {3,4-Dihydroxyphenylacetic Acid,4-Dihydroxyphenylacetic Acid: metabolism,Analysis of Variance,Animal model,Animals,Brain,Brain: anatomy & histology,Brain: metabolism,Cerebral Cortex,Cerebral Cortex: metabolism,Corpus Striatum,Corpus Striatum: metabolism,Deer mouse,Dopamine,Dopamine: metabolism,Glutathione,Glutathione: metabolism,Homovanillic Acid,Homovanillic Acid: metabolism,Obsessive–compulsive disorder,Oxidative stress,Oxidative Stress,Oxidative Stress: physiology,Oxidoreductases,Oxidoreductases: metabolism,Peromyscus,Peromyscus: metabolism,Peromyscus: physiology,Regression Analysis,Stereotyped Behavior,Stereotyped Behavior: physiology,Stereotypy,Superoxide dismutase,Superoxide Dismutase,Superoxide Dismutase: metabolism},
  number = {3-4},
  options = {useprefix=true}
}

@article{gun_lim_exploratory_2010,
  title = {An Exploratory Study of the Weather and Calendar Effects on Tourism Web Site Usage},
  author = {Gun Lim, Gyoo and Hyun Kim, Do and Choi, Minnseok and Choi, Jin H. and Chang Lee, Kun},
