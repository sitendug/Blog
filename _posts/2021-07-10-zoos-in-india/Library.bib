@article{miranda_estimation_2019,
  title = {Estimation of {{Genetic Parameters}} for {{Peak Yield}}, {{Yield}} and {{Persistency Traits}} in {{Murciano}}-{{Granadina Goats Using Multi}}-{{Traits Models}}},
  author = {Miranda, Judith C. and León, José M. and Pieramati, Camillo and Gómez, Mayra M. and Valdés, Jesús and Barba, Cecilio},
  date = {2019-07},
  journaltitle = {Animals},
  volume = {9},
  pages = {411},
  doi = {10.3390/ani9070411},
  url = {https://www.mdpi.com/2076-2615/9/7/411},
  urldate = {2019-11-06},
  abstract = {This paper studies parameters of a lactation curve such as peak yield (PY) and persistency (P), which do not conform to the usual selection criteria in the Murciano-Granadina (MG) breed, but are considered to be an alternative to benefit animal welfare without reducing production. Using 315,663 production records (of 122,883 animals) over a period of 24 years (1990\&ndash;2014), genetic parameters were estimated with uni-, bi- and multivariate analysis using multiple trait derivative free restricted maximum likelihood (MTDFREML). The heritability (h2)/repeatability (re) of PY, yield (Y) and P was estimated as 0.13/0.19, 0.16/0.25 and 0.08/0.09 with the uni-trait and h2 of bi- and multi-traits analysis ranging from 0.16 to 0.17 of Y, while that of PY and Y remained constant. Genetic correlations were high between PY\&ndash;Y (0.94 \&plusmn; 0.011) but low between PY\&ndash;P (\&ndash;0.16 \&plusmn; 0.054 to \&ndash;0.17 \&plusmn; 0.054) and between Y\&ndash;P (\&ndash;0.06 \&plusmn; 0.058 to \&ndash;0.05 \&plusmn; 0.058). Estimates of h2/re were low to intermediate. The selection for Y\&ndash;PY or both can be implemented given the genetic correlation between these traits. PY\&ndash;P and Y\&ndash;P showed low to negligible correlation values indicating that if these traits are implemented in the early stages of evaluation, they would not be to the detriment of PY\&ndash;Y. The combination of estimated breeding values (EBVs) for all traits would be a good criterion for selection.},
  keywords = {\#nosource,accuracy,breeding-values,correlations,heritability,lactation},
  langid = {english},
  number = {7}
}

