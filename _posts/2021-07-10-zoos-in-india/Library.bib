@article{yehuda_glucocorticoid-related_2014,
  title = {Glucocorticoid-Related Predictors and Correlates of Post-Traumatic Stress Disorder Treatment Response in Combat Veterans},
  author = {Yehuda, Rachel and Pratchett, Laura C. and Elmes, Matthew W. and Lehrner, Amy and Daskalakis, Nikolaos P. and Koch, Erin and Makotkine, Iouri and Flory, Janine D. and Bierer, Linda M.},
  date = {2014},
  journaltitle = {Interface focus},
  volume = {4},
  pages = {20140048--20140048},
  issn = {2042-8898 (Print)\textbackslash r2042-8898 (Linking)},
  doi = {10.1098/rsfs.2014.0048},
  url = {http://rsfs.royalsocietypublishing.org/cgi/doi/10.1098/rsfs.2014.0048},
  abstract = {The identification of biomarkers for post-traumatic stress disorder (PTSD) and resilience/recovery is critical for advancing knowledge about pathophysiology and treatment in trauma-exposed persons. This study examined a series of glucocorticoid-related biomarkers prior to and in response to psychotherapy. Fifty-two male and female veterans with PTSD were randomized 2 : 1 to receive either prolonged exposure (PE) therapy or a weekly minimal attention (MA) intervention for 12 consecutive weeks. Psychological and biological assessments were obtained prior to and following treatment and after a 12-week naturalistic follow-up. Response was defined dichotomously as no longer meeting criteria for PTSD at post-treatment based on the Clinician Administered PTSD Scale for DSM-IV (CAPS). Clinical improvement on the CAPS was apparent for both PE and MA, with no significant difference according to treatment condition. Biomarkers predictive of treatment gains included the BCLI polymorphism of the glucocorticoid receptor gene. Additional predictors of treatment response were higher bedtime salivary cortisol and 24 h urinary cortisol excretion. Pre-treatment plasma dehydroepiandrosterone/cortisol ratio and neuropetide Y (NPY) levels were predictors of reductions in PTSD symptoms, and, for NPY only, of other secondary outcomes as well, including anxiety and depression ratings. Glucocorticoid sensitivity changed in association with symptom change, reflecting clinical state. It is possible to distinguish prognostic and state biomarkers of PTSD using a longitudinal approach in the context of treatment. Identified markers may also be relevant to understanding mechanisms of action of symptom reduction.},
  file = {/home/sitendu/zoterostorage/storage/2SPM73NA/20140048.full_2.pdf;/home/sitendu/zoterostorage/storage/X73U834K/20140048.full.pdf},
  number = {5}
}

