@article{stanton_maternal_2015,
  title = {Maternal {{Behavior}} and {{Physiological Stress Levels}} in {{Wild Chimpanzees}} ({{Pan}} Troglodytes Schweinfurthii)},
  author = {Stanton, Margaret A. and Heintz, Matthew R. and Lonsdorf, Elizabeth V. and Santymire, Rachel M. and Lipende, Iddi and Murray, Carson M.},
  date = {2015},
  journaltitle = {International Journal of Primatology},
  volume = {36},
  pages = {473--488},
  issn = {0164-0291 (Print)\textbackslash r0164-0291 (Linking)},
  doi = {10.1007/s10764-015-9836-2},
  abstract = {Individual differences in maternal behavior toward, and investment in,\textbackslash noffspring can have lasting consequences, particularly among primate taxa\textbackslash ncharacterized by prolonged periods of development over which mothers can\textbackslash nexert substantial influence. Given the role of the neuroendocrine system\textbackslash nin the expression of behavior, researchers are increasingly interested\textbackslash nin understanding the hormonal correlates of maternal behavior. Here, we\textbackslash nexamined the relationship between maternal behavior and physiological\textbackslash nstress levels, as quantified by fecal glucocorticoid metabolite (FGM)\textbackslash nconcentrations, in lactating chimpanzees, Pan troglodytes\textbackslash nschweinfurthii, at Gombe National Park, Tanzania. After accounting for\textbackslash ntemporal variation in FGM concentrations, we found that mothers\textbackslash ninteracted socially (groomed and played) with and nursed their infants\textbackslash nmore on days when FGM concentrations were elevated compared to days when\textbackslash nFGM concentrations were within the range expected given the time of\textbackslash nyear. However, the proportion of time mothers and infants spent in\textbackslash ncontact did not differ based on FGM concentrations. These results\textbackslash ngenerally agree with the suggestion that elevated GC concentrations are\textbackslash nrelated to maternal motivation and responsivity to infant cues and are\textbackslash nthe first evidence of a hormonal correlate of maternal behavior in a\textbackslash nwild great ape.},
  file = {/home/sitendu/zoterostorage/storage/RE7S4M7H/108393745.pdf},
  keywords = {Glucocorticoids,Maternal investment,Mother–infant,Stress},
  number = {3}
}

@article{stanton_standardized_2015,
  title = {A Standardized Ethogram for the Felidae: {{A}} Tool for Behavioral Researchers},
  author = {Stanton, Lauren Ashley and Sullivan, Matthew Stephen and Fazio, Jilian Marie},
  date = {2015},
  journaltitle = {Applied Animal Behaviour Science},
  volume = {173},
  pages = {3--16},
  doi = {10.1016/j.applanim.2015.04.001},
  url = {http://dx.doi.org/10.1016/j.applanim.2015.04.001},
  abstract = {Standardized ethograms offer many practical benefits to behavioral researchers, and several examples exist today for various species and taxa. Despite historic evidence that suggests the family Felidae share similar behavioral repertoires, no standardized ethogram providing comprehensive behavioral definitions exists. In order to create a working ethogram for the Felidae, we conducted a thorough literature review of published articles and books containing behavioral definitions designed for felid species. A total of 95 documents qualified for inclusion, and each was evaluated to identify the terminology used in its behavioral definitions, along with any categorization implemented. The articles included the behaviors of 30 species and 40 subspecies of felids, with the most frequent single study species being the domestic cat (Felis catus), followed by several "big cats". The results were organized into the following mutually exclusive groups for comparison: domestic cat studies, big cat studies, and small cat studies excluding domestic cats (i.e. small exotic cats). Systematic review of definitions confirmed that researchers tend to define felid behavior in similar manners, although some divergence was found between the inclusion of behaviors in domestic and exotic (non-domestic) cat studies. Information from the literature review was used to create a standardized, universal ethogram for use in future felid behavioral studies. The final ethogram suggests the use of "base behaviors" which can be altered using pre-defined modifiers in order to accommodate the requirements of individual studies while retaining consistent terminology. Common behavioral categories are also defined, and suggestions of behaviors that qualify within each category are presented to further assist researchers when developing their study. The ethogram was designed to be user-friendly with clear definitions for each behavior, which should be coherent to a diverse range of observers. We anticipate that use of this ethogram will save researchers time and effort in creating behavioral definitions for their study, while also assisting in unifying felid behavioral research.},
  file = {/home/sitendu/zoterostorage/storage/SAAISLIP/Stantonetal_2015.pdf},
  keywords = {Behavior,Cat,Definition,Ethogram,Felidae,Review}
}

