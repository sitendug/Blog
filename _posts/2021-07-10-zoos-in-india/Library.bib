@article{mea_ecosystems_2005,
  ids = {mea_ecosystems_2005-1},
  title = {Ecosystems and Human Well-Being. {{Health}} Synthesis},
  author = {{MEA}},
  date = {2005},
  journaltitle = {Ecosystems},
  volume = {5},
