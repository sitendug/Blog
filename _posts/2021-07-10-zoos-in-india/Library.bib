@article{pontzer_great_2009,
  ids = {pontzer_great_2008},
  title = {Great Ranging Associated with Greater Reproductive Investment in Mammals.},
  author = {Pontzer, Herman and Kamilar, Jason M},
  date = {2009-01},
  journaltitle = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {106},
  pages = {192--6},
  doi = {10.1073/pnas.0806105106},
  url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2629235&tool=pmcentrez&rendertype=abstract},
  abstract = {Most animals must travel to find food, incurring an unavoidable energy and time cost. Economic theory predicts, and experimental work confirms, that within species, increasing the distance traveled each day to find food has negative fitness consequences, decreasing the amount of energy invested in maintenance, repair, and reproduction. Here, we show that this relationship between daily distance traveled and reproductive success is fundamentally different between species and over evolutionary time in many lineages. Phylogenetically controlled analyses of 161 eutherian mammals indicate that, after controlling for body mass, evolutionary increases in the daily distance traveled are associated with corresponding increases in both total fertility (number of offspring per lifetime) and total offspring mass (grams of offspring per lifetime). This suggests that over evolutionary time, increasing travel distance is often part of a strategy for procuring more food energy and not necessarily a response to decreased food availability. These results have important implications for ecological comparisons among species, including assessments of habitat quality based on locomotor behavior.},
  file = {/home/sitendu/zoterostorage/storage/APLQLIVI/Great ranging associated with greater reproductive investment in mammals. Pontzer and Kamilar. PNAS 2008.pdf;/home/sitendu/zoterostorage/storage/Y5HJR52Y/Great ranging associated with greater reproductive investment in mammals. Pontzer and Kamilar. PNAS 2008.pdf},
  keywords = {Animals,Biological Evolution,Body Size,Feeding Behavior,Feeding Behavior: physiology,Fertility,Litter Size,Locomotion,Locomotion: physiology,Mammals,Phylogeny,Reproduction,Reproduction: physiology,Species Specificity},
  number = {1}
}

