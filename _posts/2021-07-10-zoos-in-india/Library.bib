@article{chundawat_size_2016,
  title = {Size Matters: {{Scale}} Mismatch between Space Use Patterns of Tigers and Protected Area Size in a {{Tropical Dry Forest}}},
  author = {Chundawat, Raghunandan Singh and Sharma, Koustubh and Gogate, Neel and Malik, Pradeep K. and Vanak, Abi Tamim},
  date = {2016},
  journaltitle = {Biological Conservation},
  volume = {197},
  pages = {146--153},
  doi = {10.1016/j.biocon.2016.03.004},
  url = {http://dx.doi.org/10.1016/j.biocon.2016.03.004},
  abstract = {India harbours the largest wild tiger population in the world and Tropical Dry Forest areas constitute the largest habitat for them. Recent extinctions, however, from two high profile tiger reserves, highlight the vulnerability of tiger in this habitat. Our examination of historic range areas for tigers shows that populations are disappearing at a faster rate in Tropical Dry Forest (64\% sites suffering local extinction in 100 years) than in any other suitable habitat in India. Focusing on data from the Tropical Dry Forest of Panna Tiger Reserve in central India, we examine the spatial ecology of the tiger population prior to its local extinction. We analyse home range sizes, overlaps and shifts, as well as the range expansion and contraction of radio-collared tigers between 1996 and 2005. In this reserve, the average annual home range sizes for both males (n = 2) and females (n = 4) were three to four times larger than those reported so far from other tropical habitats in India - male: mean 179.3 ± 11.8 km2 (95\% Fixed kernel; n = 7); female: mean 46.6 ± 3.7 km2; (95\% Fixed kernel; n = 16). Adult female home ranges were exclusive and overlapped little with neighbouring female ranges (3 ± 1.46\%, n = 6). Male home ranges were not exclusive: resident floater males shared space with territorial males and mated with resident females. Home ranges of all breeding radio-collared tigers extended beyond the protected area boundary and were exposed to edge effects that exist at the periphery and outside. With such spatial use patterns, security and management measures provided within the boundary are unlikely to be very successful in protecting the population. Protected Areas in Tropical Dry Forest across India are relatively small (366.92 ± 422.12 km2 SD) and historical trends point towards a scale-mismatch that exists between the size of Protected Areas and the space use requirements of tigers. This scale mismatch adds to the vulnerability of existing small populations and perhaps explains why tiger populations in Tropical Dry Forest have disappeared at a faster rate than in any other tiger habitat of the sub-continent.},
  file = {/home/sitendu/zoterostorage/storage/KTWHC7GC/Tiger Space Use Panna 2016.pdf},
  keywords = {Central India,Edge effect,Floaters,Home range,Reserve size,Tiger conservation}
}

