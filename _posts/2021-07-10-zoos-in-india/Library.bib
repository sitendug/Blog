@article{von_glaserfeld_piaget_1974,
  ids = {von_glaserfeld_piaget_1974-1},
  title = {Piaget and the {{Radical Constructivist Epistemology}}},
  author = {Von Glaserfeld, Ernst},
  date = {1974},
  journaltitle = {Epistemology and Education.},
  pages = {1--24},
  doi = {10.23826/2014.02.094.107},
  abstract = {No original abstract. This paper discusses how Piaget's work was extended to radical constructivism, and describes the basics of radical constructivist theory. Von Glasersfeld discusses several times a particular passage of Piaget's which he quotes, in which Piaget is speaking of the time between when a child is born and when he or she learns to speak: "he is for all practical purposes but one element or entity among others in a universe that he has gradually constructed himself, and which hereafter he will experience as external to himself" (p. 2). Von Glasersfeld says that most readers take this as a metaphor, but instead we should understand it literally. He contrasts Piaget's views with the 'realist' understanding of knowledge, saying "ever since Plato, the activity of knowing or cognizing has been viewed as a kind of copying or replicating" (p. 4), but insists that "Piaget approached the problem of cognition from an altogether different angle" (p. 5). From this he derives his own views, saying that his thinking is called radical constructivism because "It holds that the knower's perceptual (and conceptual) activity is not merely one of selecting or transforming cognitive structures by means of some interaction with existing structures, but rather a constitutive activity which, alone, is responsible for every type or kind of structure an organism comes to know" (p. 6). Again, "The constructivist remains aware of the experiencer's role as an originator of all structures" (p. 7). Remaining sections discuss 'the environment as a black box,' knowledge and equilibration', and a summary and conclusion.},
  file = {/home/sitendu/zoterostorage/storage/3ESC3GFD/2014.02.094.107.pdf;/home/sitendu/zoterostorage/storage/QICY7Y5B/2014.02.094.107.pdf}
}

