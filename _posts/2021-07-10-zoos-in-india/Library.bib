@article{hann_overcoming_2007,
  title = {Overcoming {{Online Information Privacy Concerns}}: {{An Information}}-{{Processing Theory Approach}}},
  author = {Hann, Il-Horn and Hui, Kai-Lung and Lee, Sang-Yong and Png, Ivan},
  date = {2007},
  journaltitle = {Journal of Management Information Systems},
  volume = {24},
  pages = {13--42},
  issn = {0769527558},
  doi = {10.2753/MIS0742-1222240202},
  url = {http://www.tandfonline.com/doi/full/10.2753/MIS0742-1222240202},
  abstract = {The advent of the Internet has made the transmission of personally identifi\textbackslash nable information more common and often unintended by the user. As personal\textbackslash ninformation becomes more accessible, individuals worry that businesses misuse the\textbackslash ninformation that is collected while they are online. Organizations have tried to mitigate this concern in two ways: (1) by offering privacy policies regarding the handling and\textbackslash nuse of personal information and (2) by offering benefi ts such as fi nancial gains or\textbackslash nconvenience. In this paper, we interpret these actions in the context of the information-\textbackslash nprocessing theory of motivation. Information-processing theories, also known\textbackslash nas expectancy theories in the context of motivated behavior, are built on the premise\textbackslash nthat people process information about behavior–outcome relationships. By doing so,\textbackslash nthey are forming expectations and making decisions about what behavior to choose.\textbackslash nUsing an experimental setting, we empirically validate predictions that the means to\textbackslash nmitigate privacy concerns are associated with positive valences resulting in an increase\textbackslash nin motivational score. In a conjoint analysis exercise, 268 participants from the United\textbackslash nStates and Singapore face trade-off situations, where an organization may only offer\textbackslash nincomplete privacy protection or some benefi ts. While privacy protections (against\textbackslash nsecondary use, improper access, and error) are associated with positive valences, we\textbackslash nalso fi nd that fi nancial gains and convenience can signifi cantly increase individuals’\textbackslash nmotivational score of registering with a Web site. We fi nd that benefi ts—monetary\textbackslash nreward and future convenience—signifi cantly affect individuals’ preferences over Web\textbackslash nsites with differing privacy policies. We also quantify the value of Web site privacy\textbackslash nprotection. Among U.S. subjects, protection against errors, improper access, and\textbackslash nsecondary use of personal information is worth \$30.49–\$44.62. Finally, our approach\textbackslash nalso allows us to identify three distinct segments of Internet users—privacy guardians,\textbackslash ninformation sellers, and convenience seekers.\textbackslash n\textbackslash nKEY WORDS AND PHRASES: conjoint analysis, expectancy theory, fi nancial reward, information\textbackslash nprivacy, online privacy, segmentation.\textbackslash nPRIVACY},
  file = {/home/sitendu/zoterostorage/storage/RZAGUTMS/10.1.1.130.1869.pdf},
  number = {2}
}

