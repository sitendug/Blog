@book{cuddy_warmth_2008,
  title = {Warmth and {{Competence}} as {{Universal Dimensions}} of {{Social Perception}}: {{The Stereotype Content Model}} and the {{BIAS Map}}},
  author = {Cuddy, Amy J.C. and Fiske, Susan T. and Glick, Peter},
  date = {2008},
  volume = {40},
  doi = {10.1016/S0065-2601(07)00002-0},
  abstract = {The stereotype content model (SCM) defines two fundamental dimensions of social perception, warmth and competence, predicted respectively by perceived competition and status. Combinations of warmth and competence generate distinct emotions of admiration, contempt, envy, and pity. From these intergroup emotions and stereotypes, the behavior from intergroup affect and stereotypes (BIAS) map predicts distinct behaviors: active and passive, facilitative and harmful. After defining warmth/communion and competence/agency, the chapter integrates converging work documenting the centrality of these dimensions in interpersonal as well as intergroup perception. Structural origins of warmth and competence perceptions result from competitors judged as not warm, and allies judged as warm; high status confers competence and low status incompetence. Warmth and competence judgments support systematic patterns of cognitive, emotional, and behavioral reactions, including ambivalent prejudices. Past views of prejudice as a univalent antipathy have obscured the unique responses toward groups stereotyped as competent but not warm or warm but not competent. Finally, the chapter addresses unresolved issues and future research directions. © 2008 Elsevier Inc. All rights reserved.},
  file = {/home/sitendu/zoterostorage/storage/99HS4B89/6032216a05d5296a64ec2e6be07e918828c8.pdf},
