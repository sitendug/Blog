@article{edwards_enrichment_2019,
  title = {Enrichment with {{Lucerne Hay Improves Sow Maternal Behaviour}} and {{Improves Piglet Survival}}},
  author = {Edwards, Lauren E. and Plush, Kate J. and Ralph, Cameron R. and Morrison, Rebecca S. and Acharya, Rutu Y. and Doyle, Rebecca E.},
  date = {2019-08},
  journaltitle = {Animals},
  volume = {9},
  pages = {558},
  doi = {10.3390/ani9080558},
  url = {https://www.mdpi.com/2076-2615/9/8/558},
  urldate = {2019-11-06},
  abstract = {This study investigated the effects of providing lucerne hay on the behaviour and the performance of sows housed in farrowing crates during farrowing and lactation. Seventy-two mixed parity sows received either 1 kg lucerne hay daily from entry into the farrowing crate (\&minus;2 d from expected farrowing date) until weaning at 17 d (lucerne group, n = 36), or received no additional enrichment (control group, n = 36). In the 18 h prior to farrowing, the sows in the lucerne treatment spent more time performing nest-building behaviour (14.8\% lucerne vs 11.1\% control, p = 0.0009) and less time sham-chewing (1.0\% lucerne vs 1.9\% control, p = 0.01) than control sows, and gave birth to fewer stillborn piglets/litter (0.1 lucerne vs 0.4 control, p = 0.027). After farrowing (Day 3), the control sows spent less time lying than the lucerne sows (26\% control vs 43\% lucerne, p \&lt; 0.05). The control sows also spent less time interacting with their piglets during early lactation compared to late lactation (25.5\% Day 5 vs 47.3\% Day 12, p \&lt; 0.05), suggesting reduced maternal behaviour in this group. The lucerne sows continued to interact with the lucerne throughout lactation, indicating that they still found the enrichment rewarding after the nesting period had ceased. Based on these results, lucerne enrichment was considered to improve sow welfare during farrowing and lactation and reduce the number of stillborn piglets.},
  file = {/home/sitendu/zoterostorage/storage/ETSJXUPA/edwards_et_al_2019_enrichment_with_lucerne_hay_improves_sow_maternal_behaviour_and_improves_piglet.pdf;/home/sitendu/zoterostorage/storage/6PR59SMI/558.html},
  keywords = {Alfalfa,anticipatory behaviour,farrowing,lactation,nesting behaviour,welfare},
  langid = {english},
  number = {8}
}

