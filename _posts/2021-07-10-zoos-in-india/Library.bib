@article{yoshimura_detection_2018,
  title = {Detection of {{Sirtuin}}-1 Protein Expression in Peripheral Blood Leukocytes in Dogs},
  author = {YOSHIMURA, Kuniko and MATSUU, Aya and SASAKI, Kai and MOMOI, Yasuyuki},
  date = {2018-07},
