@article{argyris_action_1985,
  ids = {argyris_action_1985-1,argyris_action_1985-2,argyris_action_1985-3},
  title = {Action {{Science}}: {{Promoting}} Learning for Action and Change},
  author = {Argyris, Chris and Putnam, Robert and McLain Smith, Diana},
  date = {1985},
  journaltitle = {Action Science},
  pages = {36--79},
  issn = {02683946},
  doi = {10.1108/02683949510093849},
  abstract = {Learning occurs whenever errors are detected and corrected, or when a match between intentions and consequences is produced for the first time. There are at least 2 ways to correct errors. One is to change the behavior (for example, reduce backbiting and bad-mouthing among individuals). The second way to correct errors is to change the underlying, or master program, that leads individuals to bad-mouth others even when they say they do not intend to do so. If actions are changed without changing the master programs individuals use to produce the actions, then the correction will either fail or will not persevere. An action science approach whereby consultant researchers can help individuals see their taken-for-granted theories, test them and then redesign their action in the light of their learning is presented.},
  file = {/home/sitendu/zoterostorage/storage/RFE2HM66/Argyris, Putnam, McLain Smith, 1985.pdf;/home/sitendu/zoterostorage/storage/XBKSJGI7/Argyris, Putnam, McLain Smith, 1985.pdf;/home/sitendu/zoterostorage/storage/YGLYP2BG/Argyris, Putnam, McLain Smith, 1985.pdf;/home/sitendu/zoterostorage/storage/ZGVM29BA/Argyris, Putnam, McLain Smith, 1985.pdf}
