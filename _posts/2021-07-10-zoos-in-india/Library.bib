  author = {Escudero-Vilaplana, Vicente and Revuelta-Herrero, Jose Luis and Collado-Borrell, Roberto and Marzal-Alfaro, Belen and Gimenez-Manzorro, Alvaro and Herranz-Alonso, Ana and Sanjurjo-Saez, María},
