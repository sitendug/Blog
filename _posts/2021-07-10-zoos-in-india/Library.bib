@article{rangarajan_gajah_2010,
  title = {Gajah: {{Securing}} the Future for Elephants in {{India}}. {{The}} Report of the {{Elephant Task Force}}},
  author = {Rangarajan, Mahesh and a. Desai, Ajay and Sukumar, Raman and Easa, Ps and Vincent, S and Ganguly, Suparna and Talukdar, Bibhab Kumar and Singh, Brijendra and Mudappa, Divya and Chowdhary, Sushant and Prasad, a.N.},
  date = {2010},
  pages = {v+179-v+179},
  url = {http://moef.nic.in/downloads/public-information/ETF_REPORT_FINAL.pdf},
  abstract = {Securing a future for the elephant in India, its continued survival in the \textbackslash nwild and its humane care in captivity constitute a major challenge.  \textbackslash nThey call for drawing on the best in our communities of knowledge and \textbackslash ngovernance.  \textbackslash nThe Task Force is crystal clear on one point. India can secure the future for \textbackslash nGajah and its forest home. \textbackslash nIt will be a challenge but one we possess the ability to surmount, provided \textbackslash nwe have the will, demonstrate the wisdom and deploy the means \textbackslash nnecessary. \textbackslash nIt is not immediate extinction as much as attrition of living spaces and the \textbackslash ntense conditions of the human-elephant encounter on the ground that \textbackslash nrequire redress.  \textbackslash nAs a long lived and sociable animal familiar to all of us since childhood, \textbackslash nelephants may seem to require little help. But the shrinking of habitat and \textbackslash nthe selective killing off of tuskers in key populations by ivory poachers are \textbackslash ncause for grave concern.  \textbackslash nElephants in captivity are close to our hearts but there are times standards \textbackslash nfall short of the humane treatment and welfare they are surely entitled to. \textbackslash nTheir care givers, Mahouts and veterinary doctors too need recognition \textbackslash nand better amenities.  \textbackslash nProject Elephant has, since 1992, done much commendable work. But its \textbackslash nsuccesses notwithstanding, it needs more than an accretion of resources.  \textbackslash nElephant habitats are under immense pressure. Rapid economic expansion and development pressures require far more attention to land use plans from an ecological perspective. New knowledge needs to be brought to \textbackslash nbear in population and habitat assessment.\textbackslash n\textbackslash nAbove all, systems of mitigation to alleviate human-elephant conflict need \textbackslash nto re-energise and be made much more accountable.  \textbackslash nTo accomplish this requires administrative overhaul and better machinery. \textbackslash nThe Task Force strongly favours new institutions and mechanisms to \textbackslash nachieve these wider objectives. \textbackslash nWe need a new National Elephant Conservation Authority (NECA) on \textbackslash nthe lines of the structure for tiger conservation. Nestled with it will be a \textbackslash nnew Consortium of Elephant Research and Estimation (CERE) who will \textbackslash ndevelop and apply the best methods for enumeration. Transparency of \textbackslash nmethods and results will uphold standards and inculcate a scientific \textbackslash ntemper. \textbackslash nAlong with similar changes at the state level, there will be a new category \textbackslash nof Elephant Landscapes. These, ten in number will include the existing \textbackslash nand proposed 32 Elephant Reserves.  \textbackslash nWhile no new reserves are proposed, there will be a consolidation of the \textbackslash nexisting reserves. Over 40 per cent of the Elephant Reserves is not under \textbackslash nProtected Area or government forest.  \textbackslash nThe Task Force favours Ecologically Sensitive Area status under the \textbackslash nEnvironment Protection Act to regulate activity that may be ecologically \textbackslash nnegative.  \textbackslash nElephant Corridors that link critical populations had already been \textbackslash nidentified prior to the Task Force by scientists, administrators and reputed \textbackslash nvoluntary organizations. We have now ranked the Elephant Corridors \textbackslash naccording to priority and feasibility for action.  Our main emphasis is on \textbackslash ninnovative methods to secure habitats beyond the Protected Areas. These \textbackslash ncould include Community or Conservation Reserves, Ecosystem Services \textbackslash npayments and conservation easements. Protected Area expansion can also \textbackslash nbe considered but so too can other measures. These will forge partnerships \textbackslash nand reinforce alliances for conservation at ground level.  \textbackslash nIt is vital to stress that elephant conservation is about combining quality \textbackslash nscience with humane administration. A mobile mega herbivore, Elephas \textbackslash nmaximus is often in sharp indirect or direct conflict with our own patterns},
  file = {/home/sitendu/zoterostorage/storage/CGD9DQM9/Force, Unknown.pdf}
}

